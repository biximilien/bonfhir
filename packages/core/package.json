{
  "name": "@bonfhir/core",
  "description": "Core package for Bonfhir.",
  "version": "0.0.0",
  "repository": "https://github.com/bonfhir/bonfhir.git",
  "license": "APACHE-2.0",
  "type": "module",
  "exports": {
    "./r4b/index.js": "./dist/r4b/index.js"
  },
  "scripts": {
    "build": "yarn clean && tsc --project tsconfig.build.json",
    "check": "prettier --check ./**/*.ts && eslint ./**/*.ts && tsc --noEmit",
    "clean": "rimraf dist/",
    "codegen": "yarn workspace @bonfhir/codegen run dev run -d \"${PWD}/../../fhir/r4b/**/*.json\" -t \"${PWD}/r4b/**/*.hbs\" --helpers \"${PWD}/template-helpers.js\" -p 'prettier --write %files%'",
    "test": "jest"
  },
  "packageManager": "yarn@3.3.1",
  "files": [
    "dist/**/*",
    "!**/*.tsbuildinfo"
  ],
  "devDependencies": {
    "@bonfhir/codegen": "*",
    "@bonfhir/eslint-config": "*",
    "@bonfhir/prettier-config": "*",
    "@bonfhir/typescript-config": "*",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/fhir": "^0.0.35",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fhirpath": "^3.3.1",
    "html-entities": "^2.3.3"
  },
  "prettier": "@bonfhir/prettier-config"
}
