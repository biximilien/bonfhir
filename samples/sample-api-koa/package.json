    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.15.3",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
